   "source": [
    "import datetime\n",
    "import pytz\n",
    "\n",
    "obj = datetime.datetime.now()\n",
    "tz = pytz.timezone('Asia/Kolkata')\n",
    "obj = tz.localize(obj)\n",
    "print(obj)\n",
    "new_tz = pytz.timezone('America/New_York')\n",
    "new_tz_time = obj.astimezone(new_tz)\n",
    "print(new_tz_time)"
   ]
